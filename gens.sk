on join:
    if {gencap::%player's uuid%} < 30:
        set {gencap::%player's uuid%} to 30
    if {gencap::%player's uuid%} is not set:
        set {gencap::%player's uuid%} to 30
    if {placedgens::%player's uuid%} is not set:
        set {placedgens::%player's uuid%} to 0
    if {multi::%player's uuid%} is not set:
        set {multi::%player's uuid%} to 1
    if {minemulti::%player's uuid%} is not set:
        set {minemulti::%player's uuid%} to 1


command /addgens <player> <integer>:
    permission: op
    trigger:
        add arg-2 to {gencap::%arg-1's uuid%}
		
		
command /removegens <player> <integer>:
    permission: op
    trigger:
        subtract arg-2 from {gencap::%arg-1's uuid%}

command /resetgens <player>:
    permission: op
    trigger:
        delete {gens::%arg-1's uuid%::*}
        delete {placedgens::%arg-1's uuid%}
        kick arg-1 due to "{@maincolor}Relog and your gens will be reset!"

command /givegen <player> <integer> [<integer>]:
    permission: options
    trigger:
        if arg-3 is set:
            gengive(arg-1, arg-3, arg-2)
            stop
        gengive(arg-1, 1, arg-2)

